{
  "name": "italki-tracker",
  "homepage": "https://patricevignola.github.io/italki-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "start-server": "node scripts/start.js",
    "start": "run-p watch-schema watch-relay start-server",
    "build": "yarn relay && node scripts/build.js",
    "prettier-lint": "prettier es6 --write src/**/*.js && eslint .",
    "watch-schema": "yarn get-schema --watch",
    "get-schema": "yarn graphql get-schema",
    "watch-relay": "yarn relay --watch",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql",
    "flow": "flow",
    "test": "jest --coverage",
    "push-coverage": "coveralls < coverage/lcov.info",
    "predeploy": "yarn run build",
    "validate": "yarn relay && run-p flow test --silent",
    "deploy": "yarn prettier-lint && yarn validate && gh-pages -d build -m 'Deploy to gh-pages'"
  },
  "lint-staged": {
    "*.{js,json,css}": [
      "pretty-quick --staged && lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn validate"
    }
  },
  "jest": {
    "setupFiles": [
      "raf/polyfill",
      "./src/setupTests"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js"
    }
  },
  "dependencies": {
    "@material-ui/core": "^1.2.0",
    "@material-ui/icons": "^1.1.0",
    "autoprefixer": "7.1.6",
    "babel-runtime": "^6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "graphql": "^0.13.2",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-dropzone": "^4.2.11",
    "react-redux": "^5.0.7",
    "react-relay": "^1.6.0",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.3.1",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "relay-runtime": "^1.6.0",
    "resolve": "1.6.0",
    "style-loader": "0.19.0",
    "styled-components": "^3.3.3",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "21.0.0",
    "babel-loader": "7.1.2",
    "babel-plugin-relay": "^1.6.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^3.1.1",
    "coveralls": "^3.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-graphql": "^2.1.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "flow-bin": "^0.73.0",
    "flow-typed": "^2.4.0",
    "gh-pages": "^1.2.0",
    "graphql-cli": "^2.16.2",
    "husky": "^1.0.0-rc.8",
    "jest-localstorage-mock": "^2.2.0",
    "lint-staged": "^7.1.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.4",
    "pretty-quick": "^1.6.0",
    "react-dev-utils": "^5.0.1",
    "redux-mock-store": "^1.5.1",
    "relay-compiler": "^1.6.0"
  }
}

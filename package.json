{
  "name": "italki-tracker",
  "homepage": "https://patricevignola.github.io/italki-tracker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "raf": "^3.4.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "prettier": "prettier es6 --write src/**/*.js",
    "lint": "eslint .",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "flow": "flow",
    "test": "jest --coverage --setupTestFrameworkScriptFile=raf/polyfill",
    "push-coverage": "coveralls < coverage/lcov.info",
    "predeploy": "yarn run build",
    "validate": "run-p lint flow build test --silent",
    "deploy": "yarn prettier && yarn validate && gh-pages -d build -m 'Deploy to gh-pages'",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier && yarn validate"
    }
  },
  "devDependencies": {
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.9.1",
    "flow-bin": "^0.73.0",
    "flow-typed": "^2.4.0",
    "gh-pages": "^1.2.0",
    "husky": "^1.0.0-rc.8",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.4"
  }
}

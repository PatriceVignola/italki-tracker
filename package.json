{
  "name": "italki-tracker",
  "homepage": "https://patricevignola.github.io/italki-tracker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "raf": "^3.4.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "prettier": "prettier es6 --write src/**/*.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "flow": "flow",
    "test": "jest --coverage --setupTestFrameworkScriptFile=raf/polyfill",
    "push-coverage": "coveralls < coverage/lcov.info",
    "predeploy": "yarn run build",
    "validate": "run-p flow build test --silent",
    "deploy": "yarn validate && gh-pages -d build -m 'Deploy to gh-pages'",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "coveralls": "^3.0.1",
    "flow-bin": "^0.73.0",
    "flow-typed": "^2.4.0",
    "gh-pages": "^1.2.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.4"
  }
}
